{
  "name": "gfirem/formidable-autocomplete",
  "description": "Add an autocomplete field to search data in your forms.",
  "type": "wordpress-plugin",
  "keywords": [
    "formidable",
    "formidable autocomplete",
    "formidable wordpress field",
    "formidable autocomplete field",
    "autocomplete"
  ],
  "homepage": "https://gfirem.com/",
  "license": "Apache License 2.0",
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.1",
    "php-coveralls/php-coveralls": "^2.0@dev",
    "squizlabs/php_codesniffer": "3.*",
    "friendsofphp/php-cs-fixer": "^2.9",
    "jakub-onderka/php-parallel-lint": "dev-master",
    "jakub-onderka/php-console-highlighter": "dev-master",
    "wp-coding-standards/wpcs": "dev-master",
    "symplify/easy-coding-standard": "^5.2@dev",
    "brainmaestro/composer-git-hooks": "dev-master"
  },
  "scripts": {
    "package": "sh .tk/package.sh gfirem_autocomplete",
    "wp-release": "sh .tk/wp_release.sh",
    "beta-release": "sh .tk/full-deploy.sh gfirem_autocomplete 846 false beta",
    "full-release": "sh .tk/full-deploy.sh gfirem_autocomplete 846 false released",
    "update-freemius": "sh .tk/update-freemius.sh ../wordpress-sdk classes/include/freemius"
  }
}
